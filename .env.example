# Blockchain Node RPC URL (e.g., Infura/Alchemy endpoint or local node)
RPC_URL=https://your-blockchain-node-url

# Private key of the wallet that will issue certificates
PRIVATE_KEY=your-private-key-here

# Deployed smart contract address
CONTRACT_ADDRESS=0xYourContractAddressHere

# (Optional) Port for the server (defaults to 4000)
PORT=4000